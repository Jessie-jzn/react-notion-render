{
  "name": "react-notion-simplify",
  "version": "1.0.4",
  "description": "Fast and accurate React renderer for Notion",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:webpack": "webpack --mode production --config webpack.config.cjs",
    "build:tsc": "tsc --emitDeclarationOnly",
    "dev": "webpack serve --mode development --config webpack.config.cjs",
    "watch": "run-p watch:* --continue-on-error",
    "watch:tsc": "tsc --watch --preserveWatchOutput --emitDeclarationOnly",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "run-p test:*",
    "test:eslint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "katex": "^0.16.9",
    "prismjs": "^1.29.0"
  },
  "dependencies": {
    "@matejmazur/react-katex": "^3.1.3",
    "clipboard-copy": "^4.0.1",
    "date-fns": "^3.3.1",
    "lodash.throttle": "^4.1.1",
    "notion-types": "^6.16.0",
    "notion-utils": "^6.16.0",
    "react-fast-compare": "^3.2.2"
  },
  "optionalDependencies": {
    "@fisch0920/medium-zoom": "^1.0.7",
    "react-pdf": "^6.2.2",
    "react-use": "^17.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/react": "^18.2.0",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react-dom": "^18.2.0",
    "@types/react-modal": "^3.16.3",
    "@types/prismjs": "^1.26.3",
    "babel-loader": "^9.2.1",
    "css-loader": "^6.11.0",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.10",
    "sass": "^1.83.1",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "author": "Jessie <https://github.com/Jessie-jzn>",
  "license": "MIT",
  "keywords": [
    "notion",
    "property",
    "react"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
