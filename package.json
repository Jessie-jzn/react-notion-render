{
  "name": "react-notion-simplify",
  "version": "1.0.20",
  "description": "Fast and accurate React renderer for Notion",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:webpack": "webpack --mode production --config webpack.config.cjs",
    "build:tsc": "tsc --emitDeclarationOnly",
    "dev": "webpack serve --mode development --config webpack.config.cjs",
    "watch": "run-p watch:* --continue-on-error",
    "watch:tsc": "tsc --watch --preserveWatchOutput --emitDeclarationOnly",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "run-p test:*",
    "test:eslint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint '**/*.{ts,tsx}' --fix",
    "prepare": "npm run build",
    "example": "webpack serve --mode development --config examples/webpack.config.cjs"
  },
  "peerDependencies": {
    "katex": "^0.16.9",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@matejmazur/react-katex": "^3.1.3",
    "clipboard-copy": "^4.0.1",
    "date-fns": "^3.3.1",
    "lodash.throttle": "^4.1.1",
    "notion-types": "^6.16.0",
    "notion-utils": "^6.16.0",
    "react-fast-compare": "^3.2.2",
    "react-image": "^4.1.0",
    "react-modal": "^3.16.1"
  },
  "optionalDependencies": {
    "@fisch0920/medium-zoom": "^1.0.7",
    "react-pdf": "^6.2.2",
    "react-use": "^17.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/lodash.throttle": "^4.1.9",
    "@types/prismjs": "^1.26.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "assert": "^2.1.0",
    "babel-loader": "^9.2.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.11.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "format-number": "^3.0.0",
    "html-webpack-plugin": "^5.6.3",
    "https-browserify": "^1.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "p-map": "^7.0.3",
    "path-browserify": "^1.0.1",
    "prettier": "^3.2.4",
    "process": "^0.11.10",
    "rimraf": "^5.0.10",
    "sass": "^1.83.1",
    "sass-loader": "^13.3.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^4.15.2"
  },
  "author": "Jessie <https://github.com/Jessie-jzn>",
  "license": "MIT",
  "keywords": [
    "notion",
    "property",
    "react"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.cjs"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "endOfLine": "lf"
  }
}
